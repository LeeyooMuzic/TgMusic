{
    "name": "Inflex Music Bot",
    "description": "A Telegram Music Player Bot, written in Python with Pyrogram and Py-Tgcalls.",
    "logo": "https://graph.org/file/d6a63fe91240f0b271f7f.jpg",
    "keywords": [
    "python3",
    "telegram",
    "bot",
    "Aviax",
    "MusicBot",
    "telegram-bot",
    "pyrogram"
    ],
    "env": {
      "API_ID": {
        "description": "Get this value from https://my.telegram.org",
        "value": "7370676",
        "required": true
      },
      "API_HASH": {
        "description": "Get this value from https://my.telegram.org",
        "value": "a69ef5a0e63ddaf883c36768585eb608",
        "required": true
      },
      "BOT_TOKEN": {
        "description": "A Bot's token from Botfather",
        "value": "",
        "required": true
      },
      "MONGO_DB_URI": {
        "description": "Get a mongodb url from https://cloud.mongodb.com.",
        "value": "",
        "required": true
      },
      "OWNER_ID": {
        "description": "The user id of user whom you would like to add as OWNER.",
        "value": "5663149546",
        "required": true
      },
      "USER_NAME": {
        "description": "The username of OWNER.",
        "value": "TeleMuzicPlayer",
        "required": true
      },
      "STRING_SESSION": {
        "description": "A Pyrogram v2 String Session .",
        "value": "",
        "required": true
      },
      "QUEUE_LIMIT": {
        "description": "Fill Queue Limit . Example - 15",
        "value": "15",
        "required": false
      },
      "HEROKU_API_KEY": {
        "description": "Your Heroku account's API key",
        "value": "HRKU-333fd081-e7fe-4bdd-b1d4-eb23325cd4ed",
        "required": false
      },
      "HEROKU_APP_NAME": {
        "description": "Your heroku app name",
        "value": "",
        "required": false
      },
      "LOG_GROUP_ID": {
        "description": "Your Log Group ID, add your bot and promote as an admin with full rights!. Do not ever try to put channel ID here.",
        "value": "-1002075786437",
        "required": true
      }
    },
    "buildpacks": [
          {
              "url": "heroku/python"
          },
          {
              "url": "heroku/nodejs"
          },
          {
              "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
          }
      ],
      "stack": "container"
  }
